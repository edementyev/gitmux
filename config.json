{
  "markers": [".git", "Cargo.toml"],
  "ignore": [
    "node_modules",
    "venv",
    "bin",
    "target",
    "debug",
    "src",
    "test",
    "tests",
    "lib",
    "docs",
    "pkg"
  ],
  "entries": [
    {
      "paths": ["$HOME/dev", "$HOME/dev/sbl/operator-catalogs/test"],
      "exclude": ["*", "keebs", "nvim", "aur"]
    },
    {
      "paths": ["$HOME/dev/keebs"],
      "depth": 1,
      "include_all": true
    },
    {
      "paths": ["$HOME/dev/sbl/rest"],
      "depth": 0,
      "include_all": true
    },
    { "paths": ["$HOME/dev/nvim/plugins"] },
    { "paths": ["$HOME/dev/nvim/neovim"], "depth": 0 },
    {
      "paths": ["$HOME/bin", "$HOME/.cache", "$HOME/.config"],
      "depth": 1,
      "include_all": true
    },
    {
      "paths": ["$HOME/.local"],
      "depth": 2,
      "include_all": true
    }
  ]
}
